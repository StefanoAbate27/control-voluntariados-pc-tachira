{
  "name": "talento-humano-pc-tachira",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm:dev:*\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && node index.js",
    "electron": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.pctachira.talentohumano",
    "productName": "Talento Humano PC TÃ¡chira",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^12.4.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "electron": "^28.2.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "mysql2": "^3.14.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "electron-builder": "^24.6.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "vite": "^4.5.0"
  }
}
